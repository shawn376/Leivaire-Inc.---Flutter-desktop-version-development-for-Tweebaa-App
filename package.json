{
  "name": "tweebaa-app",
  "version": "0.1.32",
  "private": true,
  "homepage": "http://./",
  "author": "hsqa <dmhqs@qq.com>",
  "build": {
    "appId": "com.tweebaa.app",
    "productName": "Tweebaa",
    "copyright": "Copyright © 2023 tweebaa",
    "files": [
      "build/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "./build/"
    },
    "extraMetadata": {
      "main": "./build/main.js"
    },
    "extends": null,
    "asar": true,
    "mac": {
      "entitlements": "./entitlements.mac.plist",
      "hardenedRuntime": true,
      "extendInfo": {
        "NSMicrophoneUsageDescription": "请允许本程序访问您的麦克风",
        "NSCameraUsageDescription": "请允许本程序访问您的摄像头复制代码"
      }
    },
    "afterSign": "./electron/notarize.js",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./build/logo-256.ico",
      "artifactName": "tweebaa${version}.${ext}",
      "publisherName": "tweebaa"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": false,
      "installerIcon": "./build/logo.ico"
    }
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix": "eslint --fix --fix-type suggestion --fix-type problem .",
    "pages": "node generator/index.js",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit",
    "watch": "pm2 start ecosystem.config.js"
  },
  "dependencies": {
    "@tencentcloud/tuiroom-engine-js": "1.0.0",
    "@vue/cli": "5.0.8",
    "ali-oss": "^6.20.0",
    "axios": "^1.6.7",
    "bignumber.js": "^9.1.2",
    "cache-manager": "5.2.4",
    "cos-wx-sdk-v5": "0.7.11",
    "decimal.js": "^10.4.3",
    "echarts": "^5.5.1",
    "electron-notarize": "1.2.2",
    "events": "3.3.0",
    "exceljs": "^4.4.0",
    "global": "4.4.0",
    "hls.js": "^1.5.15",
    "html2canvas": "1.4.1",
    "iodash": "^0.0.1-security",
    "json2xls": "^0.1.2",
    "lodash": "^4.17.21",
    "lodash-es": "4.17.21",
    "lossless-json": "^4.0.1",
    "lru-cache": "^11.0.0",
    "mitt": "3.0.0",
    "moment": "2.29.4",
    "mqtt": "3.0",
    "node-gyp": "^9.4.1",
    "num2capital": "1.0.5",
    "pages": "^0.0.16",
    "paypal-rest-sdk": "^1.8.1",
    "pinia": "2.0.28",
    "rtc-beauty-plugin": "0.1.0",
    "sass": "1.26.5",
    "sass-loader": "8.0.2",
    "simple-html-parser": "0.0.7",
    "sqlite3": "^5.1.7",
    "string-random": "0.1.3",
    "tcplayer.js": "^5.1.0",
    "text-decoding": "1.0.0",
    "thorui-uni": "1.9.0",
    "tim-js-sdk": "2.26.2",
    "tim-upload-plugin": "1.0.6",
    "tim-wx-sdk": "2.20.1",
    "trtc-js-sdk": "^4.15.1",
    "tsignaling": "1.0.2",
    "tslib": "^2.7.0",
    "typescript": "4.4.4",
    "unplugin-auto-import": "0.12.0",
    "unplugin-vue-components": "0.22.11",
    "vue": "3.2.4",
    "vue-head": "2.2.0",
    "vue-i18n": "9.2.2",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "vuex": "4.0.0",
    "vuex-persistedstate": "4.1.0",
    "weex-bindingx": "^0.0.49",
    "xlsx": "^0.18.5",
    "zrender": "^5.6.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/runtime-corejs3": "7.17.2",
    "@dcloudio/types": "3.3.3",
    "@types/mocha": "10.0.1",
    "@types/uni-app": "1.4.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-loader": "9.1.3",
    "copy-webpack-plugin": "11.0.0",
    "crypto-js": "4.1.1",
    "electron": "22.0.0",
    "electron-builder": "23.6.0",
    "electron-is-dev": "2.0.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.2.0",
    "pm2": "5.2.0",
    "prettier": "2.8.8",
    "vite-plugin-eslint": "1.7.0",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "description": "## 项目运行",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/tweebaadev/tweebaa-app.git"
  },
  "keywords": [],
  "license": "ISC"
}
